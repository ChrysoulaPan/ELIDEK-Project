<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
</head>

<body>
    <h2>Phone numbers of selected organization:</h2>
    <div class="container">
        <div class="row" id="row">
            <div class="col-md-12">
                <div class="card" id="card-container">
                    <div class="card-body" id="card">
                    <% if (num.length == 0) { %> 
                        <h1 style="margin-top: 5rem;">No numbers found!</h1>
                    <% } else { %>
                        <div class="table-responsive">
                            <table class="table">
                                <tbody>
                                    <% num.forEach((num, _) => { %>
                                        <tr>
                                            <td><%= num.phone_number %></td>
                                        </tr>
                                    <% }) %>
                                </tbody>
                            </table>
                        </div>
                    <% } %>
                    </div>
                </div>
            </div>
        </div>
    </div>
</body>
